  ],
  "author": "UmamiAppearance",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/UmamiAppearance/SHAObj/issues",
    "email": "mail@umamiappearance.eu"
  },
  "homepage": "https://github.com/UmamiAppearance/SHAObj#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.1.0",
    "base-ex": "^0.6.1",
    "eslint": "^8.29.0",
    "http-server": "^14.1.1",
    "no-bro-cote": "^0.2.7",
    "rollup": "^3.7.1"
  }
}
