{
  "name": "sha-obj",
  "version": "1.0.0",
  "description": "JavaScript implementation of SHA (1/256/384/512) checksum calculation for the browser and node.",
  "browser": "./dist/BrowserSHAObj.esm.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint ./src/*",
    "lint:fix": "eslint ./src/* --fix",
